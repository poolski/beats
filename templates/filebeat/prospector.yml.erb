        <%= key -%>: <%= value %>
      <%- end -%>
<%- if @encoding -%>      encoding: <%= @encoding %><% end %>
<%- if @ignore_older -%>      ignore_older: <%= @ignore_older %><% end %>
<%- if @scan_frequency -%>      scan_frequency: <%= @scan_frequency %><% end %>
